    "version": "0.2.0",
    "configurations": [
        {
            "name": "Godot Debugger",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "D:/Program/Godot_v4.0/Godot_v4.0-beta3_mono_win64.exe",
            "args": [
                "--path",
                "${workspaceFolder}",
                "--verbose"
            ],
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Godot(当前场景)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "D:/Program/Godot_v4.0/Godot_v4.0-beta3_mono_win64.exe",
            "args": [
                "--path",
                "${workspaceFolder}",
                "${fileDirname}/${fileBasenameNoExtension}.tscn"
            ],
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "preLaunchTask": "build"
        }
    ]
